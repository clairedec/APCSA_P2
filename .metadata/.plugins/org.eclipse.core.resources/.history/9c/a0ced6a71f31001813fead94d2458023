import java.util.Scanner;
import static java.lang.System.*;

public class FancyWord
{
	private char[][] mat;

	public FancyWord()
	{
		mat=new char[0][0];

	}

   public FancyWord(String s)
	{
	   String word=s;
	   for ( int i=0; i<word.length();i++)
	   {
		   mat[0][i]=word.charAt(i);
		   
	   }




	}

	public String toString()
	{
		char[][] output;
		output=mat;




		return output+"\n\n";
	}
}