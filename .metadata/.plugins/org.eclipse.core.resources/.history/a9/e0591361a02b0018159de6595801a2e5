import static java.lang.System.*;

public class Word18e implements Comparable<Word18e>
{
	private String word;

	public Word18e( String s )
	{
		word=s;


	}
	
	private int Vowel()
	{
		String vowel = "AEIOUaeiou";
		int counter=0;
		for ( int i = 0; i<word.length(); i++)
		{
			for ( int j=0; j<vowel.length(); j++)
			{
				if ( word.charAt(i) == vowel.indexOf(charAt(j)))
				{
					counter++;
				}
			}
		}
	}
	
	public int getLength()
	{
		return word.length(); 
	}
	

	public int compareTo( Word18e rhs )
	{
		Word18e test = (Word18e)rhs;
		if ( getLength() > rhs.getLength())
		{
			return 1;
		}
		else if (getLength()<rhs.getLength())
		{
			return -1;
		}
		else
		{
			return toString().compareTo(test.toString());
		}
	}

	public String toString()
	{
		return word;
	}
}