import java.lang.System;
import java.lang.Math;
import java.util.Scanner;

public class Grades
{
	//instance variables
	private int num;
	private int[] grade;
	

	//constructor
	public Grades(int nums, int[] grades)
	{
		setGrades(nums, grades);
	}


	//set method
	public setGrades(int nums, int[] grades)
	{
		num=nums;
		grade=grades;
	}
	


	private double getSum()
	{
		double sum=0.0;



		return sum;
	}

	public double getAverage()
	{
		double average=0.0;



		return average;
	}


	//toString method



}